	"targetName": "bdwgc-d",
	"targetPath": "build",
	"toolchainRequirements": {
		"frontend": ">=2.111.0"
	},
	"buildTypes": {
		"debug": {
			"buildOptions": [
				"debugMode",
				"debugInfo",
				"debugInfoC"
			]
		},
		"release": {
			"buildOptions": [
				"inline",
				"optimize"
			]
		},
		"release-debug": {
			"buildOptions": [
				"debugInfo",
				"inline",
				"optimize"
			]
		},
		"release-nobounds": {
