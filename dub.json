				"releaseMode",
				"inline",
				"noBoundsCheck",
				"optimize"
			]
		}
	},
	"dflags-ldc": [
		"-i",
		"-preview=all",
		"-P-I$PACKAGE_DIR/bdwgc-master/include",
		"--verrors-context"
	],
	"dflags-dmd": [
		"-i",
		"-preview=all",
		"-P-I$PACKAGE_DIR/bdwgc-master/include",
		"-verrors=context"
	],
	"dflags-gdc": [
		"-fpreview=all",
		"-I$PACKAGE_DIR/bdwgc-master/include"
	],
	"lflags-posix": [
		"-L$PACKAGE_DIR/build"
	],
	"lflags-windows": [
		"/LIBPATH:$PACKAGE_DIR\\build"
	],
	"libs": [
		"gc"
	],
	"libs-windows": [
		"user32"
	],
